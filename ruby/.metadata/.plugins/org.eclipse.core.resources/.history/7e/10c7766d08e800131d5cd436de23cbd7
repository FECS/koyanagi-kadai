require 'net/http'
require 'uri'

class Hello
  def sayHallo
    p "Hello World!"
  end

  def showGoogle
    getHtml('http://www.example.com/index.html')
    http://animemap.net/api/table/tokyo.json
  end

  private

  def showHtml html
    Net::HTTP.get_print getHtml(html)
  end

  def getHtml html
    URI.parse html
  end
end

h=Hello::new
h::sayHallo
h.showGoogle()
